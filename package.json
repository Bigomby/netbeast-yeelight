    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
