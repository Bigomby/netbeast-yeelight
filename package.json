  "license": "GPL-3.0",
  "description": "Netbeast plugin for Yeelight devices",
  "main": "server/server.js",
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./client\" \"nodemon ./server/server.js\" ",
    "tsc:client": "tsc -p ./client",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "cd client; tsc"
  },
  "netbeast": {
    "bootOnLoad": true,
    "type": "plugin"
  },
